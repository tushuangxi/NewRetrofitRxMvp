<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xdw.retrofitrxmvpdemo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".libding.application.PadApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--.libding.rerxmvp.view.activity.MainActivity-->
        <activity android:name=".libding.rerxmvp.view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".libding.rerxmvp.view.activity.BlankActivity" />
        <activity
            android:name=".libding.rerxmvp.view.activity.NewsDetailActivity"
            android:label="@string/title_activity_news_detail"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>